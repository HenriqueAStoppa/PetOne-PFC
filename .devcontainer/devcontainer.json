{
  "name": "PetOne Codespace",
  "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
  "features": {
    "ghcr.io/devcontainers/features/java:1": {
      "version": "17",
      "installMaven": "true"
    }
  },
  "forwardPorts": [8080],
  "postCreateCommand": "java -version && mvn -v",
  "customizations": {
    "vscode": {
      "extensions": [
        "vscjava.vscode-java-pack",
        "vmware.vscode-boot-dev-pack",
        "redhat.vscode-xml",
        "humao.rest-client"
      ]
    }
  }
}
